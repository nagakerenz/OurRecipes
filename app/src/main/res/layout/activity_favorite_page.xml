<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".FavoritePage"
    android:padding="10dp">

    <ImageButton
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_baseline_arrow_back_24"
        android:background="@null" />

    <TextView
        android:id="@+id/text_Search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:paddingLeft="10dp"
        android:text="Favorites"
        android:textStyle="bold"
        android:textAlignment="center"
        android:textSize="28sp"
        android:fontFamily="arial"
        android:textColor="@color/black"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:layout_gravity="center">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="2"
            android:layout_marginBottom="30dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="5dp">

            <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="180dp"
            android:layout_height="200dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:background="@null"
            android:scaleType="centerCrop"
            app:layout_column="0"
            app:layout_row="0"
            app:srcCompat="@drawable/food_eggtoast" />

            <ImageButton
                android:id="@+id/food_loveWhite"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_row="0"
                android:layout_column="0"
                android:layout_marginLeft="149dp"
                android:layout_marginTop="23dp"
                android:background="@null"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/food_love" />

            <ImageButton
                android:id="@+id/imageButton1"
                android:layout_width="180dp"
                android:layout_height="200dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:background="@null"
                android:scaleType="centerCrop"
                app:layout_column="0"
                app:layout_row="1"
                app:srcCompat="@drawable/food_eggtoast" />

            <ImageButton
                android:id="@+id/food_love"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_row="0"
                android:layout_column="1"
                android:layout_marginLeft="148dp"
                android:layout_marginTop="23dp"
                android:background="@null"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/food_love" />


            <ImageButton
                android:id="@+id/imageButton2"
                android:layout_width="180dp"
                android:layout_height="200dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:background="@null"
                android:scaleType="centerCrop"
                app:layout_column="0"
                app:layout_row="2"
                app:srcCompat="@drawable/food_eggtoast" />

            <ImageButton
                android:id="@+id/food_love2"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_row="1"
                android:layout_column="0"
                android:layout_marginLeft="148dp"
                android:layout_marginTop="23dp"
                android:background="@null"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/food_love" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="180dp"
                android:layout_height="200dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:background="@null"
                android:scaleType="centerCrop"
                app:layout_column="0"
                app:layout_row="2"
                app:srcCompat="@drawable/food_eggtoast" />

            <ImageButton
                android:id="@+id/food_love3"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_row="1"
                android:layout_column="1"
                android:layout_marginLeft="148dp"
                android:layout_marginTop="23dp"
                android:background="@null"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/food_love" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="180dp"
                android:layout_height="200dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:background="@null"
                android:scaleType="centerCrop"
                app:layout_column="0"
                app:layout_row="2"
                app:srcCompat="@drawable/food_eggtoast" />

            <ImageButton
                android:id="@+id/food_love4"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_row="2"
                android:layout_column="0"
                android:layout_marginLeft="148dp"
                android:layout_marginTop="23dp"
                android:background="@null"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/food_love" />

            <ImageButton
                android:id="@+id/imageButton5"
                android:layout_width="180dp"
                android:layout_height="200dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:background="@null"
                android:scaleType="centerCrop"
                app:layout_column="0"
                app:layout_row="2"
                app:srcCompat="@drawable/food_eggtoast" />

            <ImageButton
                android:id="@+id/food_love5"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:layout_row="2"
                android:layout_column="1"
                android:layout_marginLeft="148dp"
                android:layout_marginTop="23dp"
                android:background="@null"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/food_love" />

        </GridLayout>
    </ScrollView>
</LinearLayout>